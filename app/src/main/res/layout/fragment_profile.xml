<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/roundbutton"
    tools:context=".bookings">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="97dp"
        android:layout_height="89dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="137dp"
        android:src="@drawable/man"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="319dp"
        android:layout_height="67dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/starterbuttton"
        android:fontFamily="@font/seogelight"
        android:gravity="center"
        android:text="Name"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="362dp"
        android:layout_height="72dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="7dp"
        android:background="@drawable/roundbutton"
        android:drawableLeft="@drawable/ic_baseline_email_24"
        android:fontFamily="@font/seogelight"
        android:gravity="center|start"
        android:paddingLeft="10dp"
        android:text="Email"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="365dp"
        android:layout_height="74dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/roundbutton"
        android:drawableLeft="@drawable/ic_baseline_bookmarks_24"
        android:fontFamily="@font/seogelight"
        android:gravity="center|start"
        android:paddingLeft="10dp"
        android:text="Booking"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="365dp"
        android:layout_height="71dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/roundbutton"
        android:drawableLeft="@drawable/ic_baseline_home_repair_service_24"
        android:fontFamily="@font/seogelight"
        android:gravity="center|start"
        android:paddingLeft="10dp"
        android:text="About Us"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <Button
        android:id="@+id/button5"
        android:layout_width="268dp"
        android:layout_height="65dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/starterbuttton"
        android:text="Sign Out"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />
</androidx.constraintlayout.widget.ConstraintLayout>